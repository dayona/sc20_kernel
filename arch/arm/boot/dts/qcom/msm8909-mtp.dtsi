			compatible = "goodix,gt9xx";
			reg = <0x5d>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <13 0x2008>;
			reset-gpios = <&msm_gpio 12 0x0>;
			interrupt-gpios = <&msm_gpio 13 0x2008>;
			power-gpios = <&msm_gpio 23 0x0>;
